<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="1200.0" style="-fx-background-color: linear-gradient( white, #999999);" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controladoresFX.FXMLProyectosController">
   <children>
      <VBox fx:id="containerCabeza" alignment="CENTER" prefHeight="200.0" prefWidth="1200.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="PROYECTOS">
               <font>
                  <Font size="96.0" />
               </font>
            </Text>
         </children>
      </VBox>
      <VBox fx:id="containerCuerpo" alignment="CENTER" layoutY="200.0" prefHeight="500.0" prefWidth="1200.0">
         <children>
            <ScrollPane hbarPolicy="NEVER" maxHeight="500.0" maxWidth="1000.0" prefHeight="500.0" prefWidth="1000.0">
               <content>
                  <VBox fx:id="proyectosContainer" alignment="TOP_CENTER" prefHeight="130.0" prefWidth="999.0" spacing="30.0">
                     <padding>
                        <Insets top="30.0" />
                     </padding>
                  </VBox>
               </content>
            </ScrollPane>
         </children>
      </VBox>
      <VBox fx:id="containerPie" alignment="CENTER" layoutY="700.0" prefHeight="100.0" prefWidth="1200.0">
         <children>
            <Button mnemonicParsing="false" onAction="#btn_crearProyecto" text="Crear proyecto">
               <font>
                  <Font size="22.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
