<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controladoresFX.FXMLModuloUnoController">
   <children>
      <VBox alignment="CENTER" prefHeight="150.0" prefWidth="1200.0">
         <children>
            <Label fx:id="labelTitulo" text="MÓDULO UNO">
               <font>
                  <Font name="System Bold" size="50.0" />
               </font>
            </Label>
         </children>
      </VBox>
      <HBox layoutY="150.0" prefHeight="500.0" prefWidth="1200.0">
         <children>
            <ScrollPane prefHeight="200.0" prefWidth="1099.0">
               <content>
                  <AnchorPane prefHeight="500.0" prefWidth="300.0" />
               </content>
            </ScrollPane>
         </children>
         <padding>
            <Insets left="50.0" right="50.0" />
         </padding>
      </HBox>
      <HBox alignment="CENTER" layoutY="650.0" prefHeight="150.0" prefWidth="1200.0" spacing="60.0">
         <children>
            <Button mnemonicParsing="false" onAction="#volverAModulos" text="Volver módulos" />
            <Button fx:id="btn_agregarActividad" mnemonicParsing="false" onAction="#BTN_agregarActividad" text="Agregar actividad" />
         </children>
      </HBox>
   </children>
</AnchorPane>
